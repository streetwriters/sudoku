<resources>
  <style name="AppThemeLight" parent="Theme.AppCompat.Light.DarkActionBar">
    <item name="colorPrimary">@color/colorPrimary</item>
    <item name="colorSecondary">@color/white</item>
    <item name="colorAccent">@color/black</item>
    <item name="colorReverseAccent">@color/white</item>
    <item name="colorBackground">@color/colorPrimary</item>
    <item name="colorScoreBoard">@color/white</item>
    <item name="colorButtonMainPage">@color/light_gray02</item>
    <item name="colorCellUnselected">@color/white</item>
    <item name="colorCellFixed">@color/light_gray02</item>
    <item name="colorCellFixedSelected">@color/gray02</item>
    <item name="colorGridSelected">@color/light_gray01</item>

    <item name="colorError">@color/error</item>
    <item name="colorCellMargin">@color/dark_gray01</item>
    <item name="colorCalenderDisabledText">@color/gray02</item>//change
    <item name="colorMainPageText">@color/gray04</item>
    <item name="colorContainerBorder">@color/light_gray03</item>
  </style>

  <style name="AppThemeDark" parent="Theme.AppCompat.Light.DarkActionBar">
    <item name="colorPrimary">@color/colorPrimaryDark</item>
    <item name="colorSecondary">@color/dark_gray03</item>
    <item name="colorAccent">@color/white</item>
    <item name="colorReverseAccent">@color/black</item>
    <item name="colorBackground">@color/dark_gray03</item>
    <item name="colorScoreBoard">@color/white</item>
    <item name="colorButtonMainPage">@color/dark_gray02</item>
    <item name="colorCellUnselected">@color/dark_gray02</item>
    <item name="colorCellFixed">@color/gray03</item>
    <item name="colorCellFixedSelected">@color/gray02</item>
    <item name="colorGridSelected">@color/gray05</item>
    <item name="colorError">@color/error</item>
    <item name="colorCellMargin">@color/dark_gray03</item>
    <item name="colorCalenderDisabledText">@color/gray02</item>
    <item name="colorMainPageText">@color/gray02</item>
    <item name="android:colorBackground">@color/dark_gray03</item>
    <item name="android:textColorPrimary">@color/white</item>
    <item name="android:textColorSecondary">@color/gray02</item>
    <item name="colorContainerBorder">@color/gray04</item>

    <item name="android:background">@color/dark_gray03</item>



  </style>

  <style name="AppThemeLight.NoActionBar">
    <item name="windowActionBar">false</item>
    <item name="windowNoTitle">true</item>
  </style>

  <style name="AppThemeDark.NoActionBar">
    <item name="windowActionBar">false</item>
    <item name="windowNoTitle">true</item>
  </style>

  <style name="AppThemeLight.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

  <style name="AppThemeLight.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>


